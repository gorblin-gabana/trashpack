{
  "name": "trashpack-wallet-extension",
  "version": "2.0.0",
  "description": "TrashPack - Wallet Chrome Extension",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "dev:extension": "cross-env NODE_ENV=development vite build --mode extension --watch",
    "build": "cross-env NODE_ENV=production vite build",
    "build:extension": "cross-env NODE_ENV=production vite build --mode extension",
    "build:all": "npm run build && npm run build:extension",
    "preview": "vite preview",
    "preview:dist": "vite preview --outDir dist",
    "clean": "rm -rf dist build",
    "generate-icons": "node generate-icons.js"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.32.1",
    "@headlessui/react": "^2.2.9",
    "@solana/kit": "^5.0.0",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "@supabase/supabase-js": "^2.87.0",
    "autoprefixer": "^10.4.22",
    "axios": "^1.13.2",
    "bip39": "^3.1.0",
    "bs58": "^6.0.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "ed25519-hd-key": "^1.3.0",
    "idb-keyval": "^6.2.2",
    "lucide-react": "^0.556.0",
    "postcss": "^8.5.6",
    "process": "^0.11.10",
    "qrcode": "^1.5.4",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hot-toast": "^2.6.0",
    "react-jdenticon": "^1.5.0",
    "react-router-dom": "^7.10.1",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.17",
    "tweetnacl": "^1.0.3",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "cross-env": "^10.1.0",
    "vite": "^7.2.7",
    "vite-plugin-node-polyfills": "^0.24.0"
  }
}
